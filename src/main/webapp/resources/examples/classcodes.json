{
  "dfe": [
    {
      "name": "root",
      "type": "table",
      "_get": "$$('.policy')",
      "tpos": {
        "x": 0,
        "y": 0
      }
    },
    {
      "name": "grp1",
      "parent": "root",
      "type": "container",
      "_get": "[$$]",
      "tpos": {
        "x": 0,
        "y": 0
      }
    },
    {
      "name": "fieldL",
      "parent": "grp1",
      "type": "label",
      "_get": "'Some label: '",
      "tpos": {
        "x": 0,
        "y": 0
      }
    },
    {
      "name": "field1",
      "parent": "grp1",
      "type": "dropdown",
      "_get": "{ value : $$('.someflag'), items: [ {value : [], description: 'Please select...'}, {value: 'Y'}, {value : 'N'}]}",
      "_set": "$$.set('.someflag', value)",
      "tpos": {
        "x": 1,
        "y": 0
      }
    },
    {
      "name": "f2_switch",
      "parent": "grp1",
      "type": "container",
      "_get": "($$('.someflag') == 'Y' ? [$$] : [])",
      "tpos": {
        "x": 2,
        "y": 0
      }
    },
    {
      "name": "field2",
      "parent": "f2_switch",
      "type": "editbox",
      "_get": "$$('.quoteid')",
      "_set": "$$.set('.quoteid', value)",
      "tpos": {
        "x": 0,
        "y": 0
      }
    },
    {
      "name": "button1",
      "parent": "root",
      "type": "button",
      "_get": "'just button'",
      "_set": "$$.append('.class')",
      "tpos": {
        "x": 0,
        "y": 1
      }
    },
    {
      "name": "classes",
      "parent": "root",
      "type": "table",
      "attributes": "{hmodel : $$}",
      "_get": "$$('.class')",
      "tpos": {
        "x": 0,
        "y": 2
      }
    },
    {
      "name": "classes_h1",
      "parent": "classes",
      "type": "label",
      "_get": "'Class Name'",
      "class": "header",
      "tpos": {
        "x": 0,
        "y": 0
      }
    },
    {
      "name": "classes_h2",
      "parent": "classes",
      "type": "label",
      "class": "header",
      "_get": "'Class Description'",
      "tpos": {
        "x": 1,
        "y": 0
      }
    },
    {
      "name": "classes_h3",
      "parent": "classes",
      "type": "label",
      "class": "header",
      "_get": "''",
      "tpos": {
        "x": 2,
        "y": 0
      }
    },
    {
      "name": "class_id",
      "parent": "classes",
      "type": "editbox",
      "_get": "$$('.code')",
      "_set": "$$.set('.code', value)",
      "tpos": {
        "x": 0,
        "y": 0
      }
    },
    {
      "name": "class_desc",
      "parent": "classes",
      "type": "editbox",
      "_get": "$$('.description')",
      "_set": "$$.set('.description', value)",
      "validation": "if( $$('.description')==0 ) $$.error = 'Description not be empty'",
      "tpos": {
        "x": 1,
        "y": 0
      }
    },
    {
      "name": "class_rb",
      "parent": "classes",
      "type": "button",
      "_get": "'Remove'",
      "_set": "$$.detach()",
      "attributes": "{ hidden: $$('policy.class').length < 2, disabled : $$('.code') != 0 }",
      "tpos": {
        "x": 2,
        "y": 0
      }
    },
    {
      "tpos": {
        "x": 0,
        "y": 1,
        "c": 3
      },
      "parent": "classes",
      "name": "class_sub",
      "type": "dropdown",
      "_get": "ajaxFeed($$, {url: 'http://localhost:8080/AJAXServlet.srv?action=getSubcodes&classCode='+$$('.code')+'&effectiveDate='+$$('..effective')+'&lob=WORK&state='+$$('..state')+'&method=WORKClassCodeScriptHelper', param: {value: $$('.subcode')}, name: 'value', element: 'result', method: 'POST', headers : {'X-Requested-With': 'Accept'}, mapper: function(v) {return { value: v.subCode, description: v.description}}})",
      "_set": "$$.set('.subcode', value)",
      "attributes": "{ default: {value: [], description: 'Please select...'}}"
    },
    {
      "tpos": {
        "x": 0,
        "y": 2,
        "c": 3
      },
      "parent": "classes",
      "name": "class_sub2",
      "type": "radiolist",
      "_get": "ajaxFeed($$, {url: 'http://localhost:8080/AJAXServlet.srv?action=getSubcodes&classCode='+$$('.code')+'&effectiveDate='+$$('..effective')+'&lob=WORK&state='+$$('..state')+'&method=WORKClassCodeScriptHelper', param: {value: $$('.subcode')}, name: 'value', element: 'result', method: 'POST', headers : {'X-Requested-With': 'Accept'}, mapper: function(v) {return { value: v.subCode, description: v.description}}})",
      "_set": "$$.set('.subcode', value)"
    },
    {
      "name": "chb1",
      "parent": "root",
      "type": "checkbox",
      "_get": "{ checked: $$.get('.someflag'), text: 'Abc'}",
      "_set": "$$.set('.someflag', value)",
      "tpos": {
        "x": 0,
        "y": 3
      }
    },
    {
      "name": "{Change Name}",
      "type": "editbox",
      "_get": "$$('.state')",
      "tpos": {
        "x": 0,
        "y": 4
      },
      "parent": "root",
      "_set": "$$.set('.state', value)"
    }
  ]
}
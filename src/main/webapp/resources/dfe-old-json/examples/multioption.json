{
  "dfe": [
    {
      "name": "root",
      "type": "div",
      "_get": "$$('policy')"
    },
    {
      "name": "field-1",
      "parent": "root", 
      "type": "multioption",
      "_get": "(function() {var o=[]; 'AK;AL;AR;AZ;CA;CO;CT;DC;DE;FL;GA;HI;IA;ID;IL;IN;KS;KY;LA;MA;MD;ME;MI;MN;MO;MS;MT;NC;ND;NE;NH;NJ;NM;NV;NY;OH;OK;OR;PA;RI;SC;SD;TN;TX;UT;VA;VT;WA;WI;WV;WY'.split(';').forEach(function(n) {o.push({value:n})}); return { value : $$('.common.ApplicableStates'), options : o }})()",
      "_set": "$$.set('.common.ApplicableStates', value)",
      "attributes": "{style : 'flex-flow:row wrap; display: flex; align-items: stretch', rowstyle : 'min-width: 70;padding: 2px;display: inline-flex;align-content: center'}"
    }
  ]
}
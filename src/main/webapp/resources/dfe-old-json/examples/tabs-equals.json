{
  "dfe": [
    {
      "name": "root",
      "type": "container",
      "_get": "$$('policy')"
    },
    {
      "name": "r1",
      "parent": "root",
      "type": "tab-s",
      "_get": "$$('.uwclassquestion')",
      "attributes": "{ haclass: 'div-tab-item-active', focusnew: 1, hfield: 'tab_header', style: 'width: 300px;', rowclass$header: 'div-tab-header', rowclass: 'div-tab-body div-alt-color2-3' }"
    },
    {
      "name": "tab",
      "parent": "r1",
      "type": "editbox",
      "_get": "$$('.question')",
      "_set": "$$.set('.question', value)",
      "validation": "if( $$('.question') == 0 ) $$.error = 'Must not be empty'",
      "attributes": "{ style : 'margin: 2px', vstrategy: 'notified'}"
    },
    {
      "name": "tab_header",
      "parent": "r1",
      "type": "div-button",
      "attributes": "{ class: 'div-button', style: 'position: relative;', errorwatch : true, vstrategy: 'notified' }",
      "_get": "$$('.questionid')",
      "validation": "$$.errorwatch()",
      "dpos": {
        "colclass": "div-tab-item"
      }
    },
    {
      "name": "rmb",
      "parent": "r1",
      "type": "button",
      "_get": "'Remove tab'",
      "_set": "$$.detach()",
      "attributes": "{ style : 'align-self: center; margin: 2px;width: fit-content' }"
    }
  ],
  "setup": []
}
{
  "dfe": [
    {
      "name": "root",
      "type": "div",
      "_get": "$$('policy.cmau.location')",
      "attributes": "{style: 'width: 600px; display: flex; flex-flow: row wrap;'}"
    },
    {
      "name": "f1",
      "type": "editbox",
      "_get": "$$('.zip')",
      "_set": "$$.set('.zip', value)",
      "dpos": {
        "colclass": "a",
        "colstyle": "b"
      },
      "parent": "root"
    },
    {
      "name": "f2",
      "type": "editbox",
      "_get": "$$('.zip')",
      "_set": "$$.set('.zip', value)",
      "dpos": {
        "colclass": "c"
      },
      "parent": "root"
    },
    {
      "name": "{Change Name}",
      "type": "editbox",
      "_get": "3",
      "dpos": {
        "colstyle": "d"
      },
      "parent": "root"
    },
    {
      "name": "hf1",
      "type": "label",
      "class": "header",
      "_get": "'Header'",
      "parent": "root"
    }
  ]
}